{{yield to="inverse"}}
{{#if showContextMenu}}
  {{#content-mask
    class="euic-menu-overlay active"
    didInsertMask=didInsertContextMenu
    action="closeContextMenu"}}
  	<nav class="context-menu-container euic-menu-container">
      {{yield contextMenuParams}}
  	</nav>
  {{/content-mask}}
{{/if}}
